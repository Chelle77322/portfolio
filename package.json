{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "A REACT Web Developer Portfolio",
  "main": "index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": " node index.js",
    "start:dev": "concurrently \"nodemon --ignore 'portfolio/*'\"\"npm run /portfolio\"",
    "portfolio": "cd portfolio && npm run start",
    "install": "cd portfolio && npm install",
    "build": "cd portfolio && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Michelle Hall",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.12.13"
  }
}
