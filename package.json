{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "A REACT portfolio",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'portfolio/*'\" \"npm run portfolio\"",
    "client": "cd portfolio/portfolio && npm run start",
    "install": "cd portfolio/portfolio && npm install",
    "build": "cd portfolio/portfolio/ && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Michelle Hall",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^4.1.0"
  },
  "dependencies": {
    "animate.css-react": "^1.1.0",
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.12.12",
    "react-bootstrap": "^1.6.1",
    "react-icons": "^4.2.0",
    "react-parallax-tilt": "^1.5.17",
    "react-responsive-carousel": "^3.2.18",
    "sass": "^1.34.1",
    "typewriter-effect": "^2.17.0"
  }
}
