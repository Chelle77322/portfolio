{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "A REACT portfolio",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'portfolio/*'\" \"npm run portfolio\"",
    "client": "cd portfolio/portfolio && npm run start",
    "install": "cd portfolio/portfolio && npm install",
    "build": "cd portfolio/portfolio/ && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chelle77322/portfolio.git"
  },
  "author": "Michelle Hall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Chelle77322/portfolio/issues"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "animate.css-react": "^1.1.0",
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.12.12"
  }
}
